      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/part_1_data_intern_test/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/part_1_data_intern_test/end" type="dateTime"/>
      <bind jr:preload="date" jr:preloadParams="today" nodeset="/part_1_data_intern_test/today" type="date"/>
      <bind nodeset="/part_1_data_intern_test/school_location/county" type="select1"/>
      <bind nodeset="/part_1_data_intern_test/school_location/school" type="select1"/>
      <bind nodeset="/part_1_data_intern_test/school_location/store_gps" type="geopoint"/>
      <bind nodeset="/part_1_data_intern_test/school_location/store_image" type="binary"/>
      <bind nodeset="/part_1_data_intern_test/school_details/school_type" type="select1"/>
      <bind nodeset="/part_1_data_intern_test/school_details/sponsor_names" type="select1"/>
      <bind nodeset="/part_1_data_intern_test/school_details/sponsor_names_other" relevant="selected( /part_1_data_intern_test/school_details/sponsor_names ,'1')" type="string"/>
      <bind nodeset="/part_1_data_intern_test/school_details/institution_type" type="select"/>
      <bind constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/number_of_females" relevant=" /part_1_data_intern_test/school_details/institution_type !=1" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/number_of_males" relevant=" /part_1_data_intern_test/school_details/institution_type !=2" type="int"/>
      <bind calculate="( /part_1_data_intern_test/school_details/number_of_females + /part_1_data_intern_test/school_details/number_of_males )" constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/total_students" type="string"/>
      <bind constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/classroom_numbers" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/teacher_numbers" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/classes_numbers" type="int"/>
      <bind constraint=".&gt;=0" nodeset="/part_1_data_intern_test/school_details/pupils_numbers" type="int"/>
      <bind nodeset="/part_1_data_intern_test/school_details/teacher_name" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/part_1_data_intern_test/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/part_1_data_intern_test/school_location">
      <label>School location</label>
      <select1 ref="/part_1_data_intern_test/school_location/county">
        <label>1. County</label>
        <item>
          <label>Nairobi</label>
          <value>nairobi</value>
        </item>
        <item>
          <label>Mombasa</label>
          <value>mombasa</value>
        </item>
        <item>
          <label>Nakuru</label>
          <value>nakuru</value>
        </item>
        <item>
          <label>Kisumu</label>
          <value>kisumu</value>
        </item>
      </select1>
      <select1 ref="/part_1_data_intern_test/school_location/school">
        <label>2. Name of school</label>
        <itemset nodeset="instance('school_name')/root/item[county= /part_1_data_intern_test/school_location/county ]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/part_1_data_intern_test/school_location/store_gps">
        <label>3. Collect the GPS coordinates of the school</label>
        <hint>GPS coordinates can only be collected when outside.</hint>
      </input>
      <upload mediatype="image/*" ref="/part_1_data_intern_test/school_location/store_image">
        <label>4. Take a photo of the school's administration block</label>
      </upload>
    </group>
    <group ref="/part_1_data_intern_test/school_details">
      <label>School Details</label>
      <select1 ref="/part_1_data_intern_test/school_details/school_type">
        <label>1. Is this school public or private?</label>
        <item>
          <label>Private</label>
          <value>private</value>
        </item>
        <item>
          <label>Public</label>
          <value>public</value>
        </item>
      </select1>
      <select1 ref="/part_1_data_intern_test/school_details/sponsor_names">
        <label>2. Who sponsors this school?</label>
        <item>
          <label>Central Government</label>
          <value>central_government</value>
        </item>
        <item>
          <label>Lovcal Government Authority</label>
          <value>local_government_authority</value>
        </item>
        <item>
          <label>NGO/CBO</label>
          <value>NGO_CBO</value>
        </item>
        <item>
          <label>Private Individual/ Organisation</label>
          <value>private_individual_organisation</value>
        </item>
        <item>
          <label>Religious Organisation</label>
          <value>religious_organisation</value>
        </item>
        <item>
          <label>Other(Specify)</label>
          <value>1</value>
        </item>
      </select1>
      <input ref="/part_1_data_intern_test/school_details/sponsor_names_other">
        <label>Other (Specify)</label>
      </input>
      <select ref="/part_1_data_intern_test/school_details/institution_type">
        <label>3.Institution type (Select all that are applicable)</label>
        <item>
          <label>Mixed School</label>
          <value>3</value>
        </item>
        <item>
          <label>Boys Only</label>
          <value>1</value>
        </item>
        <item>
          <label>Girls Only</label>
          <value>2</value>
        </item>
        <item>
          <label>Day Only</label>
          <value>4</value>
        </item>
        <item>
          <label>Boarding Only</label>
          <value>5</value>
        </item>
      </select>
      <input ref="/part_1_data_intern_test/school_details/number_of_females">
        <label>4. What is the number of female students in the school?</label>
      </input>
      <input ref="/part_1_data_intern_test/school_details/number_of_males">
        <label>5. What is the number of male students in the school?</label>
      </input>
      <input ref="/part_1_data_intern_test/school_details/classroom_numbers">
        <label>7. How many classrooms in total does the school have?</label>
      </input>
      <input ref="/part_1_data_intern_test/school_details/teacher_numbers">
        <label>8. How many teachers work at this school?</label>
      </input>
      <input ref="/part_1_data_intern_test/school_details/classes_numbers">
        <label>9. How many classes are in the school?</label>
      </input>
      <input ref="/part_1_data_intern_test/school_details/pupils_numbers">
        <label>a. How many pupils are in each class?</label>
      </input>
      <input ref="/part_1_data_intern_test/school_details/teacher_name">
        <label>b. What is the name of each class teacher?</label>
      </input>
    </group>
  </h:body>
</h:html>
